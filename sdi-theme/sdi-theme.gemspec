# sdi-theme.gemspec
Gem::Specification.new do |s|
  s.name          = "sdi-theme"
  s.version       = "0.1.0"
  s.authors       = ["Roman"]
  s.email         = [""]
  s.summary       = "Jekyll theme for SDI"
  s.description   = "This theme provides a custom layout, includes, and styles for Jekyll sites."
  s.license       = "MIT"
  s.files         = Dir["**/*"]
  s.homepage      = "https://your-theme-homepage.com"
  s.add_dependency "jekyll", ">= 3.0"
end
